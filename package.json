    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "seamless-immutable": "^7.1.4",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "TZ=UTC react-scripts test --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject",
    "lint-js": "eslint 'src/**/*.{js,jsx}'",
    "lint-snap": "eslint 'src/**/*.snap'",
    "lint-ts": "tslint -c tslint.json --project ./tsconfig.json --format stylish 'src/**/*.{ts,tsx}'"
  },
  "tsd": {
    "directory": "src/test-d"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix -c tslint.json --project ./tsconfig.json",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint"
    ],
    "*.{json,css,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{snap, test.tsx}": [
      "eslint"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.1.1",
    "redux-mock-store": "^1.5.4",
    "redux-testkit": "^1.0.6"
  }
}
