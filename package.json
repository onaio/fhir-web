{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-regular-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@onaio/connected-private-route": "^0.0.11",
    "@onaio/connected-reducer-registry": "^0.0.3",
    "@onaio/gatekeeper": "^0.1.0",
    "@onaio/loaders": "^0.0.1",
    "@onaio/redux-reducer-registry": "^0.0.9",
    "@onaio/session-reducer": "^0.0.11",
    "@onaio/utils": "^0.0.1",
    "@opensrp/server-logout": "^0.0.1",
    "@opensrp/server-service": "^0.0.4",
    "bootstrap": "^4.5.2",
    "client-oauth2": "^4.3.3",
    "connected-react-router": "^6.8.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "reactstrap": "^8.5.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "seamless-immutable": "^7.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "TZ=UTC react-scripts test --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject",
    "lint": "eslint 'src/**/*.{ts,tsx}' --max-warnings 0",
    "coveralls": "set CI=true && yarn test --runInBand --verbose --coverage=true --forceExit --detectOpenHandles && cat ./coverage/lcov.info | coveralls"
  },
  "tsd": {
    "directory": "src/test-d"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix --max-warnings 0",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix --max-warnings 0",
      "git add"
    ],
    "*.{json,js,jsx,ts,tsx,css,scss,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{snap, test.tsx}": [
      "eslint"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/index.tsx",
      "!src/serviceWorker.ts",
      "!**/tests/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/bootstrap": "^4.5.0",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/enzyme-to-json": "^1.5.3",
    "@types/jest": "^26.0.13",
    "@types/node": "^14.6.4",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.0",
    "@types/react-redux": "^7.1.9",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/reactstrap": "^8.5.1",
    "@types/redux-mock-store": "^1.0.2",
    "@types/redux-testkit": "^1.0.5",
    "@types/seamless-immutable": "^7.1.13",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "babel-eslint": "^10.1.0",
    "coveralls": "^3.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "enzyme-to-json": "^3.5.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react": "^1.1.7",
    "eslint-config-typestrict": "^1.0.0",
    "eslint-plugin-jest": "^24.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-sonarjs": "^0.5.0",
    "husky": "^4.2.5",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^10.3.0",
    "prettier": "^2.1.1",
    "redux-mock-store": "^1.5.4",
    "redux-testkit": "^1.0.6",
    "typescript": "~3.7.2"
  },
  "resolutions": {
    "react-scripts/jest": "25.3.0"
  }
}
