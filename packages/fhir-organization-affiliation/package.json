{
  "name": "@opensrp/fhir-organization-affiliation",
  "version": "0.0.1",
  "bugs": {
    "url": "https://github.com/opensrp/web/issues"
  },
  "scripts": {
    "jest": "jest --coverage --verbose --color",
    "test": "cd ../../ && yarn test packages/fhir-organization-affiliation --verbose --collectCoverage=true && cd packages/fhir-organization-affiliation",
    "test:watch": "cd ../../ && yarn test packages/fhir-organization-affiliation --verbose --watch && cd packages/fhir-organization-affiliation",
    "tsc": "tsc",
    "lint": "eslint ./**/*.{js,jsx,ts,tsx}",
    "transpile": "babel src -d dist --root-mode upward --extensions .ts,.tsx --ignore '**/*.test.ts,**/*.test.tsx,**/tests,**/__tests__'",
    "copy": "copyfiles -u 1 \"./src/**/*.{css,html}\" \"./dist/\"",
    "build": "yarn run tsc && yarn run transpile && yarn run copy",
    "extractTranslations": "yarn i18next 'src/**/*.{ts,tsx}' -c ../../i18next-parser.config.js"
  },
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "FHIR Organization Affiliation resource package",
  "main": "dist/index.js",
  "types": "dist/types",
  "files": [
    "dist",
    "locales"
  ],
  "dependencies": {
    "@opensrp/notifications": "^0.0.4",
    "@opensrp/pkg-config": "^0.0.9",
    "@opensrp/react-utils": "^0.0.10",
    "@opensrp/reducer-factory": "^0.0.13",
    "@opensrp/server-service": "^0.0.16",
    "@smile-cdr/fhirts": "^1.2.5",
    "fhirclient": "2.3.11",
    "i18next": "^19.8.4",
    "react-i18next": "^11.8.10",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@onaio/redux-reducer-registry": "^0.0.9",
    "@onaio/utils": "^0.0.1",
    "@opensrp/store": "^0.0.9",
    "@types/uuid": "8.3.0",
    "i18next-parser": "^3.5.0",
    "nock": "^13.0.6"
  },
  "peerDependencies": {
    "i18next": "^19.8.4",
    "react": "^16.13.1",
    "react-query": "^3.15.1"
  }
}
