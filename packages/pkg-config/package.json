{
  "name": "@opensrp/pkg-config",
  "version": "0.0.9",
  "description": "stores packages' configs",
  "main": "dist/index.js",
  "types": "dist/types",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/opensrp/web",
  "config": {
    "folderName": "pkg-config"
  },
  "scripts": {
    "test": " cd ../../ && yarn test packages/$npm_package_config_folderName --verbose --collectCoverage=true && cd packages/$npm_package_config_folderName",
    "tsc": "tsc",
    "lint": "eslint ./**/*.{js,jsx,ts,tsx}",
    "copy": "copyfiles -u 1 \"./src/**/*.{css,html}\" \"./dist/\"",
    "build": "yarn run tsc && yarn run transpile && yarn run copy",
    "transpile": "babel src -d dist --root-mode upward --extensions .ts,.tsx --ignore '**/*.test.ts,**/*.test.tsx,**/tests,**/__tests__'"
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "../../setupTests"
    ]
  },
  "bugs": {
    "url": "https://github.com/opensrp/web/issues"
  },
  "dependencies": {
    "react-hooks-global-state": "^1.0.1"
  },
  "devDependencies": {
    "antd": "^4.6.3"
  },
  "author": "OpenSRP Engineering",
  "license": "Apache-2.0"
}
