  "dependencies": {
    "@helsenorge/core-utils": "^21.3.0-prerelease",
    "@helsenorge/designsystem-react": ">=1.0.0-beta70",
    "@helsenorge/skjemautfyller": "^7.11.0-prerelease2",
    "@helsenorge/toolkit": "^21.3.0-prerelease",
    "dompurify": "^2.0.8",
    "fhirpath": "^2.7.3",
    "html-react-parser": "^1.4.3",
