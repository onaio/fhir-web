{
  "name": "@opensrp/store",
  "version": "0.0.3",
  "description": "App Store initializer for OpennSRP web",
  "main": "dist/index.js",
  "types": "dist/types",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/opensrp/web",
  "scripts": {
    "jest": "jest --coverage --verbose --color",
    "test": "cd ../../ && jest test  packages/keycloak-service --verbose --collectCoverage=true && cd packages/keycloak-service",
    "test:watch": "cd ../../ && jest test  packages/keycloak-service --verbose --watch && cd packages/keycloak-service",
    "tsc": "tsc",
    "lint": "eslint './**/*.{js,jsx,ts,tsx}'",
    "transpile": "babel src -d dist --root-mode upward --extensions '.ts,.tsx' --ignore '**/*.test.ts,**/*.test.tsx,**/tests,**/__tests__'",
    "copy": "copyfiles -u 1 \"./src/**/*.{css,html}\" \"./dist/\"",
    "build": "yarn run tsc && yarn run transpile && yarn run copy",
    "transpile:windows": "babel src -d dist --root-mode upward --extensions .ts,.tsx --ignore '**/*.test.ts,**/*.test.tsx,**/tests,**/__tests__'"
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "../../setupTests"
    ]
  },
  "bugs": {
    "url": "https://github.com/opensrp/web/issues"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.161",
    "@types/react-redux": "^7.1.9",
    "@types/seamless-immutable": "^7.1.13"
  },
  "dependencies": {
    "@onaio/connected-reducer-registry": "^0.0.3",
    "@onaio/gatekeeper": "^0.1.0",
    "@onaio/redux-reducer-registry": "^0.0.9",
    "@onaio/session-reducer": "^0.0.11",
    "@onaio/utils": "^0.0.1",
    "connected-react-router": "^6.8.0",
    "fast_array_intersect": "^1.1.0",
    "lodash": "^4.17.20",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "seamless-immutable": "^7.1.4"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5"
  },
  "author": "Ona Engineering",
  "license": "Apache-2.0",
  "private": false
}
