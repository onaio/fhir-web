{
  "name": "@opensrp/fhir-team-management",
  "version": "0.0.5",
  "description": "Team management components for opensrp web",
  "main": "dist/index.js",
  "types": "dist/types",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/OpenSRP/web/",
  "scripts": {
    "jest": "jest --coverage --verbose --color",
    "test": "cd ../../ && yarn test packages/fhir-team-management --verbose --collectCoverage=true -u && cd packages/fhir-team-management",
    "test:watch": "cd ../../ && yarn test packages/fhir-team-management --verbose --watch && cd packages/fhir-team-management",
    "tsc": "tsc",
    "lint": "eslint ./**/*.{js,jsx,ts,tsx}",
    "transpile": "babel src -d dist --root-mode upward --extensions .ts,.tsx --ignore '**/*.test.ts,**/*.test.tsx,**/tests,**/__tests__'",
    "copy": "copyfiles -u 1 \"./src/**/*.{css,html}\" \"./dist/\"",
    "build": "yarn run tsc && yarn run transpile && yarn run copy"
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "../../setupTests"
    ]
  },
  "bugs": {
    "url": "https://github.com/OpenSRP/web/issues"
  },
  "author": "OpenSRP Engineering",
  "license": "Apache-2.0",
  "dependencies": {
    "@onaio/utils": "^0.0.1",
    "@opensrp/fhir-location-management": "0.0.1",
    "@opensrp/notifications": "^0.0.5",
    "@opensrp/pkg-config": "^0.0.9",
    "@opensrp/react-utils": "^0.0.12",
    "@opensrp/user-management": "^0.1.19",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@smile-cdr/fhirts": "1.2.5",
    "@types/uuid": "8.3.1"
  },
  "peerDependencies": {
    "@opensrp/i18n": "^0.0.1",
    "@opensrp/store": "^0.0.10",
    "antd": "^5.5.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-query": "^3.15.1",
    "react-redux": "^7.2.1",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.2.1"
  }
}
